{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/static/css/(.*)",
      "dest": "/build/static/css/$1"
    },
    {
      "src": "/static/js/(.*)", 
      "dest": "/build/static/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/build/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/build/js/$1" 
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/build/fonts/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/build/img/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|map))",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
